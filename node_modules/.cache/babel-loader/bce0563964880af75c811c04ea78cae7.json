-col>\n    <b-col\n      sm=\"5\"\n      class=\"d-flex justify-content-center align-items-center right-register\"\n    >\n      <div class=\"col-8\">\n        <h2 class=\"text-center mb-5 title-login\">Prijavi se</h2>\n        <b-form>\n          <b-form-group label=\"ime\" label-for=\"name\">\n            <b-form-input\n              id=\"name\"\n              type=\"text\"\n              placeholder=\"Vaše ime i prezime\"\n              autocomplete=\"off\"\n              v-model.trim=\"$v.form.name.$model\"\n              :state=\"getValidation('name')\"\n            ></b-form-input>\n          </b-form-group>\n\n          <b-form-group label=\"E-mail\" label-for=\"email\">\n            <b-form-input\n              id=\"email\"\n              type=\"email\"\n              placeholder=\"vaš email@email.com\"\n              autocomplete=\"off\"\n              v-model.trim=\"$v.form.email.$model\"\n              :state=\"getValidation('email')\"\n            ></b-form-input>\n          </b-form-group>\n\n          <b-form-group label=\"Lozinka\" label-for=\"password\">\n            <b-form-input\n              id=\"password\"\n              type=\"password\"\n              placeholder=\"unesite lozinku\"\n              v-model.trim=\"$v.form.password.$model\"\n              :state=\"getValidation('password')\"\n            ></b-form-input>\n          </b-form-group>\n\n          <b-form-group label=\"ponovite lozinku\" label-for=\"confirmPassword\">\n            <b-form-input\n              id=\"confirmPassword\"\n              type=\"password\"\n              placeholder=\"Potvrdite lozinku\"\n              v-model.trim=\"$v.form.confirmPassword.$model\"\n              :state=\"getValidation('confirmPassword')\"\n            ></b-form-input>\n          </b-form-group>\n\n          <b-button type=\"button\" variant=\"primary\" block @click=\"register\"\n            ><i class=\"fas fa-sign-in-alt\"></i> Registriraj</b-button\n          >\n\n          <hr />\n\n          <b-button\n            type=\"button\"\n            variant=\"outline-secondary\"\n            block\n            @click=\"goToLogin\"\n            ><i class=\"fas fa-arrow-left\"></i> Vrati se</b-button\n          >\n        </b-form>\n      </div>\n    </b-col>\n  </b-row>\n</template>\n\n<script>\nimport { required, minLength, email, sameAs } from \"vuelidate/lib/validators\";\nimport UsersModel from \"@/models/UsersModel\";\nimport ToastMixin from \"@/mixins/toastMixin.js\";\n\nexport default {\n  mixins: [ToastMixin],\n\n  data() {\n    return {\n      form: {\n        name: \"\",\n        email: \"\",\n        password: \"\",\n        confirmPassword: \"\",\n      },\n    };\n  },\n\n  validations: {\n    form: {\n      name: {\n        required,\n        minLength: minLength(3),\n      },\n\n      email: {\n        required,\n        email,\n      },\n\n      password: {\n        required,\n        minLength: minLength(6),\n      },\n\n      confirmPassword: { \n        required, \n        sameAsPassword: sameAs('password') \n      }\n    },\n  },\n\n  methods: {\n    register() {\n      this.$v.$touch();\n      if (this.$v.$error) return;\n\n      const user = new UsersModel(this.form);\n      user.save();\n\n      this.showToast(\"uspijeh\", \"uspješno obavljeno\");\n      this.goToLogin();\n    },\n\n    getValidation(field) {\n      if (this.$v.form.$dirty === false) {\n        return null;\n      }\n\n      return !this.$v.form[field].$error;\n    },\n\n    goToLogin() {\n      this.$router.push({ name: \"login\" });\n    },\n  },\n};\n</script>\n\n<style>\n*,\n*:after,\n*:before {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  text-decoration: none;\n}\n\n.row-login {\n  margin-left: 0 !important;\n}\n\n.img-register {\n  width: 600px;\n  height: 600px;\n}\n\n.right-register {\n  background-color: #f2f2f2;\n}\n\n.title-login {\n  font-weight: bold;\n}\n</style>\n"]}]}